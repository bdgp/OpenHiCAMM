<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.web.WebView?>

<VBox id="vbox" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.bdgp.OpenHiCAMM.ReportDialog">
   <children>
      <WebView id="webView" fx:id="webView" prefHeight="1024.0" prefWidth="2000.0" VBox.vgrow="ALWAYS" />
      <HBox fillHeight="false" maxHeight="-Infinity" prefHeight="25.0" prefWidth="200.0" VBox.vgrow="NEVER">
         <children>
            <ChoiceBox fx:id="selectReport" maxHeight="-Infinity" prefHeight="50.0" prefWidth="600.0" HBox.hgrow="NEVER" />
            <Button fx:id="selectButton" mnemonicParsing="false" onAction="#selectButtonPressed" prefHeight="27.0" prefWidth="134.0" text="Select Report" HBox.hgrow="NEVER" />
            <Button fx:id="debugButton" mnemonicParsing="false" onAction="#debugButtonPressed" prefHeight="27.0" prefWidth="161.0" text="Debug Report" />
            <TextField fx:id="evaluateJs" onAction="#evaluateJs" prefColumnCount="45" style="visibility: true;" />
         </children>
      </HBox>
   </children>
</VBox>
